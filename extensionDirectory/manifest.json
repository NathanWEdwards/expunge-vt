{
    "name": "ExpungeVT!",
    "version": "1.1",
    "description": "Expunge records found on VTCourtsOnline with ease!",
    "permissions": ["activeTab", 
                    "declarativeContent", 
                    "storage",
                    "https://secure.vermont.gov/*",
                    "http://secure.vermont.gov/*",
                    "https://secure.vermont.gov/",
                    "http://secure.vermont.gov/",
                    "downloads"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      }
    },
    "icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://unpkg.com/ https://cdn.jsdelivr.net https://code.jquery.com https://cdnjs.cloudflare.com https://stackpath.bootstrapcdn.com https://use.fontawesome.com; object-src 'self'",
    "externally_connectable": {
      "ids": ["*"]
    }
  }
